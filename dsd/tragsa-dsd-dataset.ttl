@prefix qb: <http://purl.org/linked-data/cube#> .
@prefix schema: <http://schema.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sdmx-attribute: <http://purl.org/linked-data/sdmx/2009/attribute#> .
@prefix sdmx-dimension: <http://purl.org/linked-data/sdmx/2009/dimension#> .
@prefix m-subj: <http://smod-fp7.github.io/measurements-subjects.ttl#> .
@prefix components: <http://smod-fp7.github.io/components/components.ttl#> .

<http://data.smartopendata.eu/sp-pt-pilot/Climatology/DSD/>
	a qb:DataStructureDefinition ;
	rdfs:comment "data structure definition for Climatology, Spanish-Portuguese pilot"@en ;
	qb:component [qb:dimension sdmx-dimension:refPeriod ;
				  qb:componentAttachment qb:DataSet ] ;
	qb:component [qb:attribute sdmx-attribute:unitMeasure] ;
	qb:component [qb:attribute dcterms:subject] ;
	qb:component [qb:attribute components:workUnit] ;
	qb:component [qb:measure schema:minValue ] ;
	qb:component [qb:measure schema:maxValue ] .

<http://data.smartopendata.eu/sp-pt-pilot/Climatology/Dataset/>
	a qb:DataSet ;
	qb:structure <http://data.smartopendata.eu/sp-pt-pilot/Climatology/DSD/> ;
	rdfs:comment "Measurements of WorkUnits aggregated for the period 1981-2010 and represented as intervals via min and max values."@en ;
	sdmx-dimension:refPeriod <http://reference.data.gov.uk/id/gregorian-interval/1981-01-01T00:00:00/P21Y> .