@prefix : <http://smod-fp7.github.io/concept-schemes/measurements-subjects.ttl#> .
@prefix cc: <http://creativecommons.org/ns#> .

:measurements-subjects a skos:ConceptScheme ;
  skos:hasTopConcept :AverageAnnualRainfallLevel, :Slope, :Runoff, :MaxAnnualTemperature, :MinAnnualTemperature, :AvgAnnualTemperature , :AnnualEvapotranspirationLevel, ::AnnualRadiationLevel, :AnnualInsolationLevel, :AnnualHumidityLevel ;
